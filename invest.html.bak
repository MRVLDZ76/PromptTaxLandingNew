    <!-- JavaScript for Logic & Charts -->
    <script>
        // --- Navigation Logic ---
        const mobileMenuBtn = document.getElementById('mobile-menu-btn');
        const mobileMenu = document.getElementById('mobile-menu');

        mobileMenuBtn.addEventListener('click', () => {
            mobileMenu.classList.toggle('hidden');
        });

        // --- Chart 1: Time Comparison (Bar Chart) ---
        const ctxTime = document.getElementById('timeComparisonChart').getContext('2d');
        const timeChart = new Chart(ctxTime, {
            type: 'bar',
            data: {
                labels: ['Manual DIY', 'Traditional CPA', 'Prompt Tax (AI)'],
                datasets: [{
                    label: 'Minutes Spent',
                    data: [240, 180, 15], // 4 hours, 3 hours, 15 mins
                    backgroundColor: [
                        'rgba(239, 68, 68, 0.7)', // Red for bad
                        'rgba(156, 163, 175, 0.7)', // Gray for neutral
                        'rgba(14, 165, 233, 0.9)'  // Sky blue for Prompt Tax
                    ],
                    borderColor: [
                        'rgb(239, 68, 68)',
                        'rgb(156, 163, 175)',
                        'rgb(14, 165, 233)'
                    ],
                    borderWidth: 1,
                    borderRadius: 5
                }]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                plugins: {
                    legend: { display: false },
                    tooltip: {
                        callbacks: {
                            label: function(context) {
                                let label = context.dataset.label || '';
                                if (label) { label += ': '; }
                                if (context.parsed.y !== null) { label += context.parsed.y + ' Minutes'; }
                                return label;
                            }
                        }
                    }
                },
                scales: {
                    y: {
                        beginAtZero: true,
                        title: { display: true, text: 'Time Spent (Minutes)' }
                    }
                }
            }
        });

        // --- Chart 2: Competitive Landscape (Radar Chart) ---
        const ctxRadar = document.getElementById('competitorRadarChart').getContext('2d');
        const radarChart = new Chart(ctxRadar, {
            type: 'radar',
            data: {
                labels: ['Speed', 'Cost Efficiency', 'Crypto Capability', 'Ease of Use', 'Accuracy'],
                datasets: [{
                    label: 'Prompt Tax',
                    data: [95, 85, 95, 90, 95],
                    fill: true,
                    backgroundColor: 'rgba(14, 165, 233, 0.2)',
                    borderColor: 'rgb(14, 165, 233)',
                    pointBackgroundColor: 'rgb(14, 165, 233)',
                    pointBorderColor: '#fff',
                    pointHoverBackgroundColor: '#fff',
                    pointHoverBorderColor: 'rgb(14, 165, 233)'
                }, {
                    label: 'Legacy Software',
                    data: [40, 70, 30, 60, 75],
                    fill: true,
                    backgroundColor: 'rgba(239, 68, 68, 0.1)',
                    borderColor: 'rgb(239, 68, 68)',
                    pointBackgroundColor: 'rgb(239, 68, 68)',
                    pointBorderColor: '#fff',
                    pointHoverBackgroundColor: '#fff',
                    pointHoverBorderColor: 'rgb(239, 68, 68)'
                }]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                elements: { line: { borderWidth: 3 } },
                scales: {
                    r: {
                        angleLines: { display: false },
                        suggestedMin: 0,
                        suggestedMax: 100
                    }
                }
            }
        });

        // --- Chart 3: Ad Strategy (Doughnut) ---
        const ctxAds = document.getElementById('adStrategyChart').getContext('2d');
        const adChart = new Chart(ctxAds, {
            type: 'doughnut',
            data: {
                labels: ['Google Ads (Search)', 'TikTok/Reels (Video)', 'Facebook/IG (Social)', 'Other'],
                datasets: [{
                    label: 'Budget Allocation',
                    data: [45, 35, 15, 5],
                    backgroundColor: [
                        '#10b981', // Emerald (Google)
                        '#db2777', // Pink (TikTok)
                        '#3b82f6', // Blue (FB)
                        '#9ca3af'  // Gray
                    ],
                    hoverOffset: 4
                }]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                plugins: {
                    legend: {
                        position: 'right',
                        labels: { color: '#cbd5e1', font: { size: 10 } }
                    }
                }
            }
        });

        // --- Investment ROI Calculator Logic ---
        const slider = document.getElementById('investmentRange');
        const displayInvestment = document.getElementById('investmentDisplay');
        const displayUsers = document.getElementById('usersDisplay');
        const displayRevenue = document.getElementById('revenueDisplay');
        const displayProfit = document.getElementById('profitDisplay');

        // Assumptions
        const CAC = 65; // Cost Per Acquisition
        const AOV = 145; // Average Order Value (Blended between products)
        const MARGIN = 0.30; // 30% Net Margin estimate

        function updateCalculator() {
            const budget = parseInt(slider.value);
            
            // Format Currency
            const formatter = new Intl.NumberFormat('en-US', {
                style: 'currency',
                currency: 'USD',
                maximumFractionDigits: 0
            });

            // Calculate Metrics
            const users = Math.floor(budget / CAC);
            const revenue = users * AOV;
            const profit = revenue * MARGIN;

            // Update DOM
            displayInvestment.textContent = formatter.format(budget);
            displayUsers.textContent = users.toLocaleString();
            displayRevenue.textContent = formatter.format(revenue);
            displayProfit.textContent = formatter.format(profit);

            // Animate Text color briefly on update
            displayRevenue.classList.add('text-sky-400');
            setTimeout(() => displayRevenue.classList.remove('text-sky-400'), 200);
        }

        // Event Listener
        slider.addEventListener('input', updateCalculator);
        
        // Initialize
        updateCalculator();

        // --- Investor Returns Calculator (NEW) ---
        const investorSlider = document.getElementById('investorAmountRange');
        const investorAmountDisplay = document.getElementById('investorAmountDisplay');
        const principalReturn = document.getElementById('principalReturn');
        const profitShare = document.getElementById('profitShare');
        const totalReturn = document.getElementById('totalReturn');
        const roiPercent = document.getElementById('roiPercent');

        // Tax Season Projections
        const TAX_SEASON_REVENUE = 3480000; // $3.48M total revenue Jan-Apr 2026
        const GROSS_MARGIN = 0.75; // 75% gross margin
        const OPERATING_EXPENSES_PERCENT = 0.25; // 25% of gross profit
        const INVESTOR_PROFIT_SHARE = 0.30; // 30% of net profit goes to investors
        const TOTAL_INVESTMENT_POOL = 500000; // Assume $500K total investment pool for calculations

        function updateInvestorCalculator() {
            const investmentAmount = parseInt(investorSlider.value);
            
            // Format Currency
            const formatter = new Intl.NumberFormat('en-US', {
                style: 'currency',
                currency: 'USD',
                maximumFractionDigits: 0
            });

            // Calculate Tax Season Economics
            const grossProfit = TAX_SEASON_REVENUE * GROSS_MARGIN; // $2.61M
            const operatingExpenses = grossProfit * OPERATING_EXPENSES_PERCENT; // $653K
            const netProfit = grossProfit - operatingExpenses; // $1.957M
            const totalProfitSharePool = netProfit * INVESTOR_PROFIT_SHARE; // $587K total to all investors
            
            // Calculate individual investor's share (proportional to investment)
            const investorSharePercent = investmentAmount / TOTAL_INVESTMENT_POOL;
            const investorProfitShare = totalProfitSharePool * investorSharePercent;
            
            // Calculate returns
            const principal = investmentAmount;
            const profit = investorProfitShare;
            const total = principal + profit;
            const roi = ((total - principal) / principal) * 100;

            // Update DOM
            investorAmountDisplay.textContent = formatter.format(investmentAmount);
            principalReturn.textContent = formatter.format(principal);
            profitShare.textContent = formatter.format(profit);
            totalReturn.textContent = formatter.format(total);
            roiPercent.textContent = `+${roi.toFixed(1)}% ROI`;
        }

        // Event Listener
        investorSlider.addEventListener('input', updateInvestorCalculator);
        
        // Initialize
        updateInvestorCalculator();

        // --- Contact Form Handler ---
        const contactForm = document.getElementById('investorContactForm');
        contactForm.addEventListener('submit', (e) => {
            e.preventDefault();
            alert('Thank you for your interest! Our team will reach out within 24 hours to discuss investment details.');
            contactForm.reset();
        });

    </script>
